<com:TRepeater ID="Repeater" EnableViewState="false">	
<prop:HeaderTemplate>	
	<table cellpadding="2">
		<tr style="color:white;background-color:black">
		  <th>contrato</th>
		  <th>num empleado</th>
		  <th>nombre</th>
		  <th>Descripcion</th>
		</prop:HeaderTemplate>
		 
		<prop:ItemTemplate>
		<tr style="background-color:#BFCFFF">
		<td><%#$this->Data['contrato']%></td>
		<td><%#$this->Data['num_empleado']%></td>
		<td><%#$this->Data['nombre']%></td>
		<td><%#$this->Data['descripcion']%></td>
		</tr>
		</prop:ItemTemplate>
		 
		<prop:AlternatingItemTemplate>
		<tr style="background-color:#E6ECFF">
		<td><%#$this->Data['contrato']%></td>
		<td><%#$this->Data['num_empleado']%></td>
		<td><%#$this->Data['nombre']%></td>
		<td><%#$this->Data['descripcion']%></td>
		</tr>
		</prop:AlternatingItemTemplate>
		 
		<prop:FooterTemplate>
		<tr style="color:white;background-color:black;text-align:center;">
		<td colspan="5">Computer Parts Inventory</td>
		</tr>
	</table>

	<table border="0">
		<tr>
		  <td align="right"><%#$this->DataItem['contrato'] %></td>
		  <td align="right"><%#$this->DataItem['num_empleado'] %></td>
		  <td align="right"><%#$this->DataItem['nombre'] %></td>
		  <td align="right"><%#$this->DataItem['descripcion'] %></td>
		</tr>
	</table>
	</prop:FooterTemplate>
</com:TRepeater>
	<com:TDataGrid ID="dgDescuentos" Font.Size="11px" AutoGenerateColumns="False" GridLines="Both" BorderStyle="Solid" Width="100%">
